{
	"name": "gridllm-worker-client",
	"version": "1.0.0",
	"description": "Distributed AI inference worker client for GridLLM server",
	"main": "dist/index.js",
	"scripts": {
		"dev": "tsx watch src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"docker:build": "docker build -t gridllm-client .",
		"docker:run": "docker run -p 3000:3000 --name gridllm-client-container gridllm-client",
		"docker:stop": "docker stop gridllm-client-container",
		"docker:clean": "docker rm gridllm-client-container",
		"docker:logs": "docker logs gridllm-client-container"
	},
	"keywords": [
		"ai",
		"ollama",
		"distributed",
		"inference",
		"broker",
		"redis",
		"typescript"
	],
	"author": "jwstanwick",
	"license": "MIT",
	"dependencies": {
		"express": "^4.18.2",
		"cors": "^2.8.5",
		"helmet": "^7.1.0",
		"compression": "^1.7.4",
		"dotenv": "^16.3.1",
		"joi": "^17.11.0",
		"bullmq": "^4.15.4",
		"ioredis": "^5.3.2",
		"node-cron": "^3.0.3",
		"axios": "^1.6.2",
		"winston": "^3.11.0",
		"uuid": "^9.0.1",
		"ws": "^8.14.2",
		"jsonwebtoken": "^9.0.2",
		"bcryptjs": "^2.4.3",
		"module-alias": "^2.2.3"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/cors": "^2.8.17",
		"@types/compression": "^1.7.5",
		"@types/node": "^20.10.4",
		"@types/uuid": "^9.0.7",
		"@types/ws": "^8.5.10",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/bcryptjs": "^2.4.6",
		"@types/jest": "^29.5.8",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"eslint": "^8.54.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.1",
		"tsx": "^4.6.2",
		"typescript": "^5.3.3",
		"nodemon": "^3.0.2"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"_moduleAliases": {
		"@": "dist"
	}
}
